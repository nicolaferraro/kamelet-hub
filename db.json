{
  "catalog": {
    "apiVersion": "v1",
    "items": [
      {
        "apiVersion": "camel.apache.org/v1alpha1",
        "kind": "Kamelet",
        "metadata": {
          "annotations": {
            "camel.apache.org/kamelet.icon": "data:image/svg+xml;base64,"
          },
          "name": "timer"
        },
        "spec": {
          "definition": {
            "description": "Produces periodic events with a custom payload",
            "properties": {
              "message": {
                "description": "The message to generate",
                "title": "Message",
                "type": "string"
              }
            },
            "required": [
              "message"
            ],
            "title": "Timer"
          },
          "flow": {
            "from": {
              "steps": [
                {
                  "set-body": {
                    "constant": "Hello [here there'll be the message] !!!"
                  }
                },
                {
                  "to": "log:info"
                }
              ],
              "uri": "timer:tick"
            }
          },
          "types": {
            "out": {
              "mediaType": "application/json",
              "schema": {
                "id": "text.camel.apache.org",
                "type": "string"
              }
            }
          }
        },
        "status": {
          "phase": "Ready"
        }
      },
      {
        "apiVersion": "camel.apache.org/v1alpha1",
        "kind": "Kamelet",
        "metadata": {
          "annotations": {
            "camel.apache.org/kamelet.icon": "data:image/svg+xml;base64,"
          },
          "name": "twitter-search"
        },
        "spec": {
          "definition": {
            "title": "Twitter Search",
            "description": "Search keywords on Twitter",
            "properties": {
              "keywords": {
                "description": "The keywords to search for",
                "title": "Keywords",
                "type": "string"
              }
            },
            "required": [
              "keywords"
            ]
          },
          "flow": {
            "from": {
              "uri": "twitter-search:keywords",
              "steps": [
                {
                  "to": "log:info"
                }
              ]
            }
          },
          "types": {
            "out": {
              "mediaType": "application/json",
              "schema": {
                "id": "twitter.search.camel.apache.org",
                "type": "object"
              }
            }
          }
        }
      },
      {
        "apiVersion": "camel.apache.org/v1alpha1",
        "kind": "Kamelet",
        "metadata": {
          "annotations": {
            "camel.apache.org/kamelet.icon": "data:image/svg+xml;base64,"
          },
          "name": "slack-message"
        },
        "spec": {
          "definition": {
            "title": "Slack Messages",
            "description": "Listen for messages in Slack",
            "properties": {
              "room": {
                "description": "The Slack room to read messages from (e.g. \"#myroom\")",
                "title": "Room",
                "type": "string"
              }
            },
            "required": [
              "room"
            ]
          },
          "flow": {
            "from": {
              "uri": "slack:#kamelets",
              "steps": [
                {
                  "marshal": {
                    "json": {}
                  }
                },
                {
                  "to": "log:info"
                }
              ]
            }
          },
          "types": {
            "out": {
              "mediaType": "application/json",
              "schema": {
                "id": "slack.message.camel.apache.org",
                "type": "object"
              }
            }
          }
        }
      }
    ],
    "kind": "List",
    "metadata": {
      "resourceVersion": "",
      "selfLink": ""
    }
  }
}
